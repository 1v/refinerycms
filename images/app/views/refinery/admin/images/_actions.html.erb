<ul>
  <li>
    <%= link_to t('.create_new_image'), main_app.new_refinery_admin_image_path(:dialog => true, :width => 600, :height => 300),
                :class => "add_icon" %>
  </li>
  <% if @design_finalised %>
    <% other_image_views.each do |image_view| %>
      <li>
        <%= link_to t('switch_to', :scope => 'refinery.admin.images.index.view', :view_name => t("#{image_view}", :scope => 'refinery.admin.images.index.view')),
                    main_app.refinery_admin_images_path(:view => image_view, :page => params[:page]),
                    :class => "reorder_icon"  %>
      </li>
    <% end %>
  <% end %>
</ul>